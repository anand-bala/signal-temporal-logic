@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/argusTargets.cmake")

set_and_check(
  argus_INCLUDE_DIR "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_INCLUDEDIR@"
)
set_and_check(argus_LIB_DIR "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@")

if(NOT argus_FIND_QUIETLY)
  message(
    STATUS
      "Found argus: ${argus_INCLUDE_DIR} (found version \"${argus_VERSION}\" ${argus_VERSION_TYPE})"
  )
endif()
